{
    "name": "solvians/php-casestudy",
    "description": "Solvians PHP Case Study",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-mongodb": "*",
        "mongodb/mongodb": ">=1.1.2",
        "laminas/laminas-escaper": "^2.9.0",
        "laminas/laminas-http": "^2.15.0",
        "laminas/laminas-router": "^3.5.0",
        "laminas/laminas-diactoros": "^2.8.0",
        "laminas/laminas-psr7bridge": "^1.5.0",
        "laminas/laminas-servicemanager": "^3.10",
        "laminas/laminas-stdlib": "^3.6.1",
        "laminas/laminas-log": "^2.13.1",
        "laminas/laminas-cli": "^1.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7",
        "laminas/laminas-test": "*",
        "michelf/php-markdown": "^1.9.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
            "AppUnitBaseTests\\": "tests/phpunit/src/"
        }
    }
}
